{
  "name": "eventhub",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:seed": "cd apps/api && npm run db:seed"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.9.2"
  },
  "pnpm": {
    "overrides": {
      "glob@7.2.3": "11.0.3",
      "inflight@1.0.6": "npm:@npmcli/fs@3.1.1"
    }
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
